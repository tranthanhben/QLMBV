{
  "name": "adjobs-front",
  "description": "Adjos Client (Frontend UI)",
  "author": "Liti Solutions <info@liti.ws>",
  "version": "0.1.0",
  "homepage": "liti.ws",
  "main": "babel.server.js",
  "scripts": {
    "start": "NODE_PATH=\"./src\" NODE_ENV=\"production\" PORT=\"8080\" node ./babel.server",
    "build": "node ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack/prod.config.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js -c .eslintrc src",
    "start-dev": "NODE_PATH=\"./src\" NODE_ENV=\"development\" node ./babel.server",
    "watch-client": "UV_THREADPOOL_SIZE=100 NODE_PATH=\"./src\" node webpack/webpack-dev-server.js",
    "dev": "node ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client\" \"npm run start-dev\""
  },
  "dependencies": {
    "babel": "5.4.7",
    "babel-plugin-typecheck": "0.0.3",
    "body-parser": "1.13.2",
    "bootstrap": "3.3.5",
    "classnames": "^2.1.3",
    "compression": "1.5.1",
    "express": "4.13.1",
    "express-session": "1.11.3",
    "file-loader": "0.8.4",
    "font-awesome": "4.3.0",
    "http-proxy": "1.11.1",
    "piping": "0.1.8",
    "react": "0.13.3",
    "react-inline-css": "1.1.1",
    "react-redux": "0.2.2",
    "react-router": "1.0.0-beta2",
    "redux": "1.0.0-rc",
    "redux-devtools": "0.1.0",
    "serialize-javascript": "1.0.0",
    "serve-favicon": "2.3.0",
    "serve-static": "1.10.0",
    "superagent": "1.2.0",
    "url-loader": "0.5.6"
  },
  "devDependencies": {
    "autoprefixer-loader": "2.0.0",
    "babel-core": "5.4.7",
    "babel-eslint": "3.1.26",
    "babel-loader": "5.1.3",
    "babel-runtime": "5.4.7",
    "concurrently": "0.1.1",
    "css-loader": "0.15.5",
    "eslint": "0.23.0",
    "eslint-config-airbnb": "0.0.6",
    "eslint-plugin-react": "2.7.1",
    "extract-text-webpack-plugin": "0.3.8",
    "json-loader": "0.5.2",
    "less": "2.5.1",
    "less-loader": "2.2.0",
    "react-a11y": "0.1.1",
    "react-hot-loader": "1.2.7",
    "strip-loader": "0.1.0",
    "style-loader": "0.12.3",
    "webpack": "1.10.5",
    "webpack-dev-server": "1.9.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
